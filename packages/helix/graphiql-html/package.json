{
  "name": "@pablosz/graphql-helix-graphiql",
  "version": "1.1.0",
  "description": "A highly evolved GraphQL HTTP Server ðŸ§¬",
  "repository": "https://github.com/contrawork/graphql-helix.git",
  "license": "MIT",
  "author": "Daniel Rearden <danielrearden@gmail.com>",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "concurrently -r pnpm:prepack",
    "bundle": "concurrently -r -m 1 pnpm:bundle:*",
    "bundle:ts": "node scripts/build-graphiql.cjs",
    "bundle:webpack": "webpack -c browser/webpack.config.js && cpy browser/css/graphiql.min.css bundle/",
    "prepack": "concurrently -r -m 1 pnpm:bundle \"bob-esbuild build\"",
    "postpublish": "gh-release"
  },
  "devDependencies": {
    "@graphql-tools/url-loader": "7.0.7",
    "@graphql-tools/utils": "8.0.2",
    "@n1ru4l/graphql-live-query": "0.7.1",
    "@types/prettier": "2.3.2",
    "@types/prop-types": "15.7.4",
    "@types/react": "17.0.16",
    "@types/react-dom": "17.0.9",
    "@types/terser-webpack-plugin": "5.0.4",
    "bob-esbuild": "^1.0.0",
    "bob-esbuild-cli": "^1.0.0",
    "changesets-github-release": "^0.0.3",
    "concurrently": "^6.2.1",
    "copy-to-clipboard": "^3.3.1",
    "cpy-cli": "^3.1.1",
    "graphiql": "^1.1.0",
    "graphql": "15.4.0",
    "graphql-language-service-interface": "^2.4.3",
    "prettier": "2.3.2",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "terser-webpack-plugin": "5.1.4",
    "ts-loader": "9.2.5",
    "typescript": "4.3.5",
    "webpack": "5.50.0",
    "webpack-cli": "4.7.2"
  },
  "publishConfig": {
    "directory": "dist"
  }
}
